<div class="container px-5 px-sm-0">
  <div class="row">
    <div class="col-md-3">
<%= render'books/list',user:@user,book:@book %>
</div>
<div class="col-md-8 offset-md-1">
 <h2>Users</h2>
 <table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>caption</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= image_tag user.get_profile_image("40X40") %></td>
      <td><%= user.name %></td>
      
    </tr>
    <% end %>
  </tbody>
  </table>
  </div>
 </div>
 </div>